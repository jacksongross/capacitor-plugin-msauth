{
  "name": "@jacksongross/capacitor-plugin-msauth",
  "version": "3.6.4",
  "description": "This plugin enables MSAL support for Capacitor.",
  "main": "dist/plugin.cjs.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "unpkg": "dist/plugin.js",
  "files": [
    "android/src/main/",
    "android/build.gradle",
    "dist/",
    "ios/Plugin/",
    "JacksongrossCapacitorPluginMsauth.podspec"
  ],
  "author": "Recognize",
  "license": "LGPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/jacksongross/capacitor-plugin-msauth.git"
  },
  "bugs": {
    "url": "https://github.com/jacksongross/capacitor-plugin-msauth/issues"
  },
  "keywords": [
    "capacitor",
    "plugin",
    "native",
    "msal",
    "microsoft",
    "azure",
    "authentication"
  ],
  "scripts": {
    "verify": "npm run verify:ios && npm run verify:android && npm run verify:web",
    "verify:ios": "cd ios && pod install && xcodebuild -workspace Plugin.xcworkspace -scheme Plugin -destination generic/platform=iOS && cd ..",
    "verify:android": "cd android && ./gradlew clean build test && cd ..",
    "verify:web": "npm run build",
    "lint": "npm run eslint && npm run prettier -- --check && npm run swiftlint -- lint",
    "fmt": "npm run eslint -- --fix && npm run prettier -- --write && npm run swiftlint -- --autocorrect --format",
    "eslint": "eslint src/. --ext ts",
    "prettier": "prettier \"{src/*.ts,**/*.java}\"",
    "swiftlint": "node-swiftlint",
    "build": "npm run clean && tsc && rollup -c rollup.config.mjs",
    "clean": "rimraf ./dist",
    "watch": "tsc --watch",
    "prepublishOnly": "npm run fmt && npm run lint && npm run build",
    "precommit": "npm run fmt && npm run lint && npm run build"
  },
  "devDependencies": {
    "@capacitor/android": "^8.1.0",
    "@capacitor/cli": "^8.1.0",
    "@capacitor/core": "^8.1.0",
    "@capacitor/ios": "^8.1.0",
    "@ionic/eslint-config": "^0.4.0",
    "@ionic/prettier-config": "^4.0.0",
    "@ionic/swiftlint-config": "^2.0.0",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.57.1",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.6.0",
    "precommit": "^1.2.2",
    "prettier": "^3.8.1",
    "prettier-plugin-java": "^2.8.1",
    "rimraf": "^6.1.3",
    "rollup": "^4.57.1",
    "swiftlint": "^2.0.0",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "@capacitor/core": ">=8.0.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "prettier": {
    "plugins": ["prettier-plugin-java"],
    "printWidth": 120,
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "quoteProps": "as-needed",
    "jsxSingleQuote": false,
    "trailingComma": "all",
    "bracketSpacing": true,
    "bracketSameLine": false,
    "arrowParens": "always",
    "overrides": [
      {
        "files": ["*.java"],
        "options": {
          "printWidth": 140,
          "tabWidth": 4,
          "useTabs": false,
          "trailingComma": "none"
        }
      },
      {
        "files": "*.md",
        "options": {
          "parser": "mdx"
        }
      }
    ]
  },
  "swiftlint": "@ionic/swiftlint-config",
  "dependencies": {
    "@azure/msal-browser": "^2.32.2"
  },
  "eslintConfig": {
    "extends": "@ionic/eslint-config/recommended"
  },
  "capacitor": {
    "ios": {
      "src": "ios"
    },
    "android": {
      "src": "android"
    }
  },
  "precommit": {
    "tasks": [
      "npm run precommit"
    ]
  }
}
